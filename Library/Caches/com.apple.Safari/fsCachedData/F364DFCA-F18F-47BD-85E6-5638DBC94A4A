<script>
  $(function(){
    $('#video_player').modal('show');
  })
  CS.videoPlayer = {
    speeds: [1, 1.5, 2, 2.5, 3],
    init: function() {
      var video = $("#homepage_video");
      var wrapper = $(video).closest('.video--sublime');
      CS.videoPlayer.initSublime(video[0]);
      CS.videoPlayer.initSpeedControls(wrapper);
    },
    initSpeedControls: function(element) {
      var speedControls = $('<select class="video-control video-control--speed"></select>')
      var speeds = CS.videoPlayer.speeds;
      for(var i=0, l=speeds.length; i<l; i++){
        speedControls.append('<option value="'+speeds[i]+'">'+speeds[i]+'</option>')
      }
      $(element).append(speedControls);
  
      speedControls.on('change', function(evt) {
        var rate = parseFloat($(evt.target).val());
        $(element).find('video:first')[0].playbackRate = rate
        CS.videoPlayer.setCookie('videoPlaybackRate', rate);
      });
    },
    initSublime: function(element) {
      if (!((element) instanceof HTMLVideoElement)) {
        console.log("Hey, how 'bout you gimme a video element, my child...");
        return false;
      }
      sublime.ready(function(){
        sublime.prepare(element, function(player){
          if ( player ) {
            var playbackRate = CS.videoPlayer.getCookie('videoPlaybackRate');
            if(playbackRate) {
              $(element.parentElement).find('video:first')[0].playbackRate = parseFloat(playbackRate);
              $(".video-control--speed").val(parseFloat(playbackRate))
            }
  
            player.on({
              start: function(player) {
                CS.Segment.track( CS.Event.Video.PLAYED );
              },
              end: function(player) {
                CS.Segment.track( CS.Event.Video.WATCHED );
              }
            });
          }
        });
      });
    },
  
    setCookie: function(key, value) {
        var expires = new Date();
        expires.setTime(expires.getTime() + (1 * 24 * 60 * 60 * 1000));
        document.cookie = key + '=' + value + ';expires=' + expires.toUTCString();
    },
  
    getCookie: function(key) {
        var keyValue = document.cookie.match('(^|;) ?' + key + '=([^;]*)(;|$)');
        return keyValue ? keyValue[2] : null;
    }
  }
  CS.videoPlayer.init()
</script>
<script>
  $(function() {
    $('.js-pathDescription').each(function() {
      CS.fixOrphanWords({ text: $(this) })
    });
  });
</script>

