    <script type="text/javascript">
        $(function () {
            var fileTemplate = "<div id=\"{{id}}\">";
            
            fileTemplate += "<div class=\"preview\"></div>";
            fileTemplate += "<div class=\"deletePic\" onclick=\"deletePic()\"></div>";
            //fileTemplate += "<div class=\"filename\">{{filename}}</div>";
            fileTemplate += "</div>";
            function slugify(text) {
                text = text.replace(/[^-a-zA-Z0-9,&\s]+/ig, '');
                text = text.replace(/-/gi, "_");
                text = text.replace(/\s/gi, "-");
                return text;
            }
            $("#SingleImage, #uploadPhoto").html5Uploader({
                postUrl: "",
                onClientLoadStart: function (e, file) {
                    deletePic(); // delete pic before uploading another in this project only

                    var upload = $("#uploadPhoto");
                    if (upload.is(":hidden")) {
                        upload.show();
                    }
                    upload.append(fileTemplate.replace(/{{id}}/g, slugify(file.name)).replace(/{{filename}}/g, file.name));
                }, onClientLoad: function (e, file) {
                    $("#" + slugify(file.name)).find(".preview").append("<img src=\"" + e.target.result + "\" alt=\"\">");
                    $("#" + slugify(file.name)).find(".preview").append("<input name='imageSrc' id='imageSrc' type='hidden' value=\"" + e.target.result + "\" alt=\"\">");
                    $("#imageSrc").val(e.target.result);
                }, onServerLoadStart: function (e, file) {
                    
                }, onServerProgress: function (e, file) {
                    
                }, onServerLoad: function (e, file, response) {
                    
                }, onSuccess: function (e, file, response) {
                    
                }
            });});
    </script>
